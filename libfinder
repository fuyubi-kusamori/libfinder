    <script>
        // タブ切り替え機能
        function openTab(tabName) {
            const tabContents = document.getElementsByClassName('tab-content');
            for (let i = 0; i < tabContents.length; i++) {
                tabContents[i].classList.remove('active');
            }
            
            const tabButtons = document.getElementsByClassName('tab-button');
            for (let i = 0; i < tabButtons.length; i++) {
                tabButtons[i].classList.remove('active');
            }
            
            document.getElementById(tabName).classList.add('active');
            document.querySelector(`.tab-button[onclick="openTab('${tabName}')"]`).classList.add('active');
        }

        // サブタブ切り替え機能
        function openSubTab(subTabName) {
            const subTabContents = document.getElementsByClassName('sub-tab-content');
            for (let i = 0; i < subTabContents.length; i++) {
                subTabContents[i].classList.remove('active');
            }
            
            const subTabButtons = document.getElementsByClassName('sub-tab-button');
            for (let i = 0; i < subTabButtons.length; i++) {
                subTabButtons[i].classList.remove('active');
            }
            
            document.getElementById(subTabName).classList.add('active');
            document.querySelector(`.sub-tab-button[onclick="openSubTab('${subTabName}')"]`).classList.add('active');
        }

        // 検索機能
        document.getElementById('search-button').addEventListener('click', searchSchools);
        document.getElementById('search-input').addEventListener('keyup', function(event) {
            if (event.key === 'Enter') {
                searchSchools();
            }
        });
        
        document.getElementById('reset-button').addEventListener('click', function() {
            document.getElementById('search-input').value = '';
            document.getElementById('search-results').innerHTML = '';
            document.getElementById('search-results').classList.add('hidden');
            
            const tabContents = document.getElementsByClassName('tab-content');
            for (let i = 0; i < tabContents.length; i++) {
                tabContents[i].style.display = tabContents[i].classList.contains('active') ? 'block' : 'none';
            }
        });

        function searchSchools() {
            const searchInput = document.getElementById('search-input').value.toLowerCase();
            if (searchInput.length < 1) return;
            
            const searchResults = document.getElementById('search-results');
            searchResults.innerHTML = '';
            searchResults.classList.remove('hidden');
            
            const links = document.querySelectorAll('.school-link');
            const foundLinks = [];
            
            links.forEach(link => {
                if (link.textContent.toLowerCase().includes(searchInput)) {
                    foundLinks.push(link.cloneNode(true));
                }
            });
            
            if (foundLinks.length > 0) {
                const resultTitle = document.createElement('h3');
                resultTitle.className = 'font-normal mb-3 text-pink-700';
                resultTitle.textContent = `検索結果: ${foundLinks.length}件`;
                searchResults.appendChild(resultTitle);
                
                const resultGrid = document.createElement('div');
                resultGrid.className = 'grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4 gap-3';
                
                foundLinks.forEach(link => {
                    resultGrid.appendChild(link);
                });
                
                searchResults.appendChild(resultGrid);
            } else {
                const noResult = document.createElement('p');
                noResult.className = 'text-center p-4 text-gray-500';
                noResult.textContent = '検索結果が見つかりませんでした';
                searchResults.appendChild(noResult);
            }
            
            // 全てのタブを非表示にする
            const tabContents = document.getElementsByClassName('tab-content');
            for (let i = 0; i < tabContents.length; i++) {
                tabContents[i].style.display = 'none';
            }
        }

        // 最初のタブをアクティブに
        document.addEventListener('DOMContentLoaded', function() {
            openTab('east-main');
            openSubTab('commercial');
        });
    </script>
</body>
</html>
